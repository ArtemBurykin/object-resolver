  "authors": [],
  "require": {
    "php":"^7.0",

    "symfony/framework-bundle":"^3.0|^4.0",
    "symfony/security":"^3.0|^4.0",
    "doctrine/orm": "^2.6",
    "doctrine/doctrine-bundle": "^1.8"
  },
  "require-dev": {
    "ext-sqlite" : "*",
    "phpunit/phpunit": "^5.4",
    "friendsofphp/php-cs-fixer": "^2.10",
